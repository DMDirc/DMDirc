# Default mode settings for Snircd
# b,k,l,imnpstrDducCN

identity.name = Snircd mode aliases
identity.ircd = snircd

server.modeD = Auditorium mode
server.moded = Auditorium mode
server.enablemodec = true
server.modeC = No channel CTCPs
server.enablemodeC = true
server.modeN = No channel notices
server.enablemodeN = true
server.modeu = No quit mesages
server.enablemodeu = true
server.modeT = No multi-target messages

server.umodex = Hide hostname
server.umodeR = Only registered users may query
