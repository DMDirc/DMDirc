# Default mode settings for Asuka
# b,k,l,imnpstrDdcCNu

identity.name = Asuka mode aliases
identity.ircd = asuka

server.modeD = Auditorium mode
server.moded = Auditorium mode
server.modeC = No channel CTCPs
server.modeN = No channel notices
server.modeu = No quit messages

server.umodeI = Hide idle time
server.umodek = Services operator
server.umoden = Hide channel list
server.umodeg = Receive debug information
server.umodeX = Extended operator