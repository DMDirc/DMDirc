# Unreal IRCd mode aliases

identity.name = Unreal mode aliases
identity.ircd = unreal

server.modeA = Admins only
server.modea = Channel admin
server.modeb = Ban
server.modec = No formatting
server.modeC = No channel CTCPs
server.modee = Exception
server.modef = Flood protection
server.modeG = G-rated
server.modeh = Half-op
server.modei = Invite only
server.modeI = Invite
server.modej = Join throttle
server.modeK = Disable /knock
server.modek = Channel key
server.model = User limit
server.modeL = Limit redirection
server.modeM = Mute non-registered users
server.modem = Moderated
server.moden = No external messages
server.modeO = IRCops only
server.modeo = Op
server.modep = Private
server.modeq = Channel owner
server.modeQ = Only servers can kick
server.modeR = Only registered users can join
server.modeS = Strip all colours
server.modes = Secret
server.modet = Only ops can set topic
server.modeT = No channel notices
server.modeu = Auditorium
server.modeV = Disable /invite
server.modev = Voice
server.modez = SSL users only
